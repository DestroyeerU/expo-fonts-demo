{
  "expo": {
    "name": "Expo Fonts Demo",
    "slug": "expo-fonts-demo",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.destroyeeru.fonts_demo"
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
